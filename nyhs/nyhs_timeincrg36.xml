      <c id="aspace_ref1_nl6" level="series">
        <did>
          <unittitle>Series I. Production Engineering General Manager William J. Carter Files on the East Texas Pulp and Paper Company</unittitle>
          <physdesc altrender="whole">
            <extent altrender="materialtype spaceoccupied">2 Linear feet</extent>
            <extent altrender="carrier">in 2 record cartons</extent>
          </physdesc>
          <unitdate type="inclusive">1947-1958</unitdate>

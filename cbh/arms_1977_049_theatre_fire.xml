    <dsc>
      <c id="aspace_ref1_a0f" level="series">
        <did>
          <unittitle>Series 1. Administrative and Financial Documents</unittitle>
          <unitdate normal="1876/1879" type="inclusive">1876-1879</unitdate>
          <langmaterial><language langcode="eng">English</language>.</langmaterial>
